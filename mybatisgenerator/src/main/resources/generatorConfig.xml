<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <classPathEntry location="C:\\Users\\lenovo\\.m2\\repository\\mysql\\mysql-connector-java\\5.1.21\\mysql-connector-java-5.1.21.jar" />

    <context id="mysql_tables" targetRuntime="MyBatis3Simple">

        <!-- 去掉mybatis生成的注释 -->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/data"
                        userId="root"
                        password="alexgaoyh">
        </jdbcConnection>

        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.alexgaoyh.model" targetProject="src/main/java">
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <javaClientGenerator type="ANNOTATEDMAPPER" targetPackage="com.alexgaoyh.dao" targetProject="src/main/java">
        </javaClientGenerator>

        <table tableName="sysman_user" domainObjectName="sysmanUser" >
            <generatedKey column="user_id" sqlStatement="Mysql"/>
        </table>

    </context>
</generatorConfiguration>